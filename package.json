{
  "name": "beginning-cloudflare-worker-react-d1",
  "description": "Cloudflare's native serverless SQL database.",
  "cloudflare": {
    "label": "Worker + D1 Database",
    "products": [
      "Workers",
      "D1"
    ],
    "categories": [
      "storage"
    ],
    "docs_url": "https://developers.cloudflare.com/d1/",
    "preview_icon_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/6c63cca8-354a-4951-45a0-d58769ac6f00/public",
    "preview_image_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/aa6158f1-7711-4485-f134-a8c802c0a800/preview",
    "publish": true
  },
  "devDependencies": {
    "typescript": "5.9.3",
    "wrangler": "4.56.0"
  },
  "private": true,
  "scripts": {
    "cf-typegen": "wrangler types",
    "check": "tsc && wrangler deploy --dry-run",
    "deploy": "wrangler deploy",
    "dev": "pnpm seedLocalD1 && wrangler dev",
    "seedRemoteD1": "wrangler d1 migrations apply DB --remote",
    "seedLocalD1": "wrangler d1 migrations apply DB --local",
    "exeLocalD1": "npx wrangler d1 execute d1-template-database --local --file=./migrations/0001_init_table.sql",
    "exeRemoteD1": "npx wrangler d1 execute d1-template-database --remote --file=./migrations/0001_init_table.sql"
  },
  "dependencies": {
    "pnpm": "^10.28.1"
  }
}